<?xml version="1.0" encoding="UTF-8"?>
<script>

	<binding fileName="modules/website/persistentdocument/import/website_binding.xml" />

	<website byTag="default_modules_website_default-website" id="default-website">
		<execute method="setDocumentIdAttribute" />
		<binding fileName="modules/store/setup/globalDefaultStructure.xml" />
		<execute method="setDocumentIdAttribute" attribute-name="website-id" />
		<execute method="setDocumentTypeAttribute" attribute-name="website-type" />
		<topic byLabel="Compte client">
			<execute method="setDocumentIdAttribute" />
			<execute method="setDocumentTypeAttribute" />
			<binding fileName="modules/store/setup/customerDefaultStructure.xml" />
		</topic>
		<topic byLabel="Nos magasins" id="storetopic" />
		<menus>
			<menu byTag="contextual_website_website_menu-main">
				<menuitempage pageid="storetopic" />
			</menu>
		</menus>
	</website>

	<binding fileName="modules/sample/setup/store/store-data.xml" />

</script>